        (obstaculo_en puerta0)
        (obstaculo_en hab2)
	(obstaculo_en puerta1)
        (obstaculo_en hab1)
    )
    (:goal
        (and
            (videollamada_terminada paciente0 hora3)
            (videollamada_terminada paciente1 hora6)
            (videollamada_terminada paciente2 hora7)
            (robot_en base)
        )
    )
)