    (:init
        (robot_en base)
        (carga_en base)
        (lugar_desinfeccion enfermeria)
        (hora_actual hora0)
        (hora_siguiente hora0 hora1)
        (hora_siguiente hora1 hora2)
        (hora_siguiente hora2 hora3)
        (hora_siguiente hora3 hora4)
        (hora_siguiente hora4 hora5)
        (hora_siguiente hora5 hora6)
        (hora_siguiente hora6 hora7)
        (hora_siguiente hora7 hora8)
        (hora_siguiente hora8 hora9)
        (hora_siguiente hora9 hora10)
        (paciente_en paciente0 hab0)
        (paciente_en paciente1 hab1)
        (paciente_en paciente2 hab2)
        (videollamada_pendiente paciente0 hora3)
        (videollamada_cancelada paciente0 hora3 hora2)
        (videollamada_pendiente paciente1 hora6)
        (videollamada_pendiente paciente2 hora7)
        (videollamada_cancelada paciente2 hora7 hora6)
